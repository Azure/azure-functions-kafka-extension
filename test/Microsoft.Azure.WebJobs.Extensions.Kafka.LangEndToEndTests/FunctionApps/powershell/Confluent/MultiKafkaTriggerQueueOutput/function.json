{
  "bindings": [
    {
      "type": "kafkaTrigger",
      "name": "kafkaEvents",
      "direction": "in",
      "brokerList": "ConfluentBrokerList",
      "topic": "e2e-kafka-powershell-multi-confluent",
      "username": "ConfluentCloudUsername",
      "password": "ConfluentCloudPassword",
      "protocol": "saslSsl",
      "authenticationMode": "plain",
      "consumerGroup": "$Default",
      "dataType": "string",
      "cardinality": "MANY"
    },
    {
      "type": "queue",
      "direction": "out",
      "name": "Msg",
      "queueName": "e2e-powershell-multi-confluent",
      "connection": "AzureWebJobsStorage"
    }
  ]
}