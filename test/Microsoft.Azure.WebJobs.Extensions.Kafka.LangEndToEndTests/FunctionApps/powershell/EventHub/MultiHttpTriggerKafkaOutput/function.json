{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "Request",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "kafka",
      "name": "Message",
      "brokerList": "EventHubBrokerList",
      "topic": "e2e-kafka-powershell-multi-eventhub",
      "username": "$ConnectionString",
      "password": "%EventHubConnectionString%",
      "protocol": "SASLSSL",
      "authenticationMode": "PLAIN",
      "direction": "out"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "Response"
    }
  ]
}